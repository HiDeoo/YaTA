{
  "name": "yata",
  "version": "1.17.4",
  "description": "Twitch chat client",
  "author": "HiDeoo",
  "license": "MIT",
  "private": true,
  "scripts": {
    "prepare": "patch-package && husky install",
    "prestart": "yarn run changelog",
    "start": "react-scripts start",
    "prebuild": "yarn run changelog",
    "build": "react-scripts build",
    "lint": "yarn run lint:ts && yarn run lint:css",
    "lint:ts": "eslint --max-warnings=0 --ext ts,tsx src && tsc --noEmit",
    "lint:css": "stylelint 'src/**/*.tsx'",
    "preanalyze": "yarn run build",
    "analyze": "source-map-explorer build/static/js/main.*",
    "changelog": "cp CHANGELOG.md src",
    "vercel": "vercel"
  },
  "dependencies": {
    "@blueprintjs/core": "3.46.0",
    "@blueprintjs/icons": "3.27.0",
    "@blueprintjs/select": "3.16.5",
    "animejs": "3.2.1",
    "bowser": "2.11.0",
    "clsx": "1.1.1",
    "copy-to-clipboard": "3.3.1",
    "file-saver": "2.0.5",
    "linkifyjs": "2.1.9",
    "localforage": "1.9.0",
    "lodash": "4.17.21",
    "marked": "2.1.3",
    "nanoid": "3.1.23",
    "node-jose": "2.0.0",
    "pluralize": "8.0.0",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-flip-toolkit": "7.0.13",
    "react-helmet-async": "1.0.9",
    "react-progressive-image": "0.6.0",
    "react-redux": "7.2.4",
    "react-router-dom": "5.2.0",
    "react-scripts": "4.0.3",
    "react-timeago": "6.2.1",
    "react-tooltip": "4.2.21",
    "react-transition-group": "4.4.2",
    "react-virtualized": "9.22.3",
    "recompose": "0.30.0",
    "redux": "4.1.0",
    "redux-persist": "6.0.0",
    "reselect": "4.0.0",
    "semver-compare": "1.0.0",
    "styled-components": "5.3.0",
    "tinycolor2": "1.4.2",
    "twitch-js": "1.2.13",
    "unistring": "https://github.com/akahuku/unistring#master"
  },
  "devDependencies": {
    "@types/animejs": "3.1.4",
    "@types/file-saver": "2.0.3",
    "@types/lodash": "4.14.171",
    "@types/marked": "2.0.4",
    "@types/node": "16.3.2",
    "@types/pluralize": "0.0.29",
    "@types/react": "17.0.14",
    "@types/react-dom": "17.0.9",
    "@types/react-redux": "7.1.18",
    "@types/react-router-dom": "5.1.8",
    "@types/react-transition-group": "4.4.2",
    "@types/react-virtualized": "9.21.12",
    "@types/recompose": "0.30.8",
    "@types/styled-components": "5.1.11",
    "@types/tinycolor2": "1.4.3",
    "@types/webpack-env": "1.16.2",
    "eslint-plugin-prettier": "3.4.0",
    "husky": "7.0.1",
    "lint-staged": "11.0.1",
    "patch-package": "6.4.7",
    "postinstall-prepare": "2.0.0",
    "prettier": "2.3.2",
    "source-map-explorer": "2.5.2",
    "stylelint": "13.13.1",
    "stylelint-config-standard": "22.0.0",
    "stylelint-config-styled-components": "0.1.1",
    "stylelint-processor-styled-components": "1.10.0",
    "typescript": "4.3.5",
    "vercel": "23.0.1"
  },
  "resolutions": {
    "@types/react": "17.0.14"
  },
  "lint-staged": {
    "*.{ts,tsx,md,json,css}": "prettier --write"
  },
  "eslintConfig": {
    "extends": "react-app",
    "plugins": [
      "prettier"
    ],
    "rules": {
      "prettier/prettier": "error",
      "import/no-anonymous-default-export": [
        "warn",
        {
          "allowObject": true
        }
      ]
    }
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "chrome 79",
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "engines": {
    "node": ">=16.x <18"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/HiDeoo/YaTA"
  },
  "keywords": [
    "twitch",
    "chat",
    "client"
  ],
  "bugs": {
    "url": "https://github.com/HiDeoo/YaTA/issues"
  },
  "contributors": [
    "nD00rn <ndoorn@outlook.com>",
    "tollus"
  ],
  "homepage": "https://yata.vercel.app"
}
